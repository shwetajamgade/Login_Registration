@model List<tbl_UserReg>
@{
    ViewBag.Title = "userList";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table table-bordered table-responsive mt-5">
    <h4>User List</h4>
    <tr class="text-center">
        <th>UserName</th>
        <th>Email</th>
        <th>Password</th>
        <th>Gender</th>
        <th>Action</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr class="text-center">
            <td>@item.UserName</td>
            <td>@item.Email</td>
            <td>@item.Password</td>
            <td>@item.Gender</td>
            <td>
                @Html.ActionLink("Edit", "UserProfile", "Home", new { id = item.UserID }, htmlAttributes: new { @class = "btn btn-outline-primary" })
                @Html.ActionLink("Delete", "DeleteUser", "Home", htmlAttributes: new { @class = "btn btn-outline-danger", id = item.UserID })
            </td>
        </tr>
    }
</table>

